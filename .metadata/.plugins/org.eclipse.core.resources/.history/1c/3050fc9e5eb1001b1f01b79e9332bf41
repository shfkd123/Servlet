    pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true" %>

<%
	Scanner in = new Scanner(System.in);
	int userNum =0;
	System.out.println("숫자를 입력하세요");
	userNum = in.next();

	for(int i=1; i <= 9; i++){
		System.out.println(userNum + "*" + i + " = " + userNum * i);
	}

%>